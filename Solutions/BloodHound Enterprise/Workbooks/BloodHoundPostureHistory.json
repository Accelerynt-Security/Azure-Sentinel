    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "BHEPostureHistoryData_CL\n| where tenant_url in~ ({bhe_tenant})\n| where domain_name in~ ({domain_name})\n| where data_type == \"findings\"\n| summarize arg_max(TimeGenerated, *) by domain_name,tenant_url\n| summarize avg(todouble(value)) by bin(metric_date, 12h), domain_name, tenant_url\n| order by metric_date asc",
        "size": 1,
        "aggregation": 2,
        "title": "Findings Trend Over Time",
        "timeContextFromParameter": "time",
